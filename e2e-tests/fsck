#!/bin/bash

. e2e-tests/help/setup

echo abc 123 > e2etmp/data/foo
echo abc 124 > e2etmp/data/foo.jpg

# Set our baseline for the test.
$TB update e2etmp/update.cfg
$TB fsck e2etmp/update.cfg

# Extra "valid" blobs are okay
echo "abc 125" > e2etmp/blobs/bb964bb682d701aad340ab78c677e597fa8fc33df131c8b48f5b951c43d73338
$TB fsck e2etmp/update.cfg

# Duplicate files are okay.
cp e2etmp/git/foo e2etmp/git/cp_of_foo
$TB fsck e2etmp/update.cfg
